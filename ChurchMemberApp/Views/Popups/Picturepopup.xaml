<?xml version="1.0" encoding="utf-8" ?>
<pages:PopupPage  xmlns:pages="http://rotorgames.com" xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ChurchMemberApp.Views.Popups.Picturepopup">
    <Grid>
        <Frame HeightRequest="400" WidthRequest="400" BackgroundColor="White" Padding="5" HorizontalOptions="Center" VerticalOptions="Center">
            <Image x:Name="image" HeightRequest="250" Aspect="AspectFill"/>
        </Frame>
        <Button Text="Cancel" Margin="0,20,0,0" Style="{StaticResource Buttons}" BackgroundColor="White" TextColor="Black" HorizontalOptions="Center" Clicked="Button_Clicked" VerticalOptions="End" />
    </Grid>

</pages:PopupPage>